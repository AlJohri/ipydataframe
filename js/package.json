{
  "name": "ipydataframe",
  "version": "0.1.0",
  "description": "Transform dataframes interactively in Jupyter notebooks",
  "author": "Pascal Bugnion",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbugnion/ipydataframe.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "update": "rimraf dist/ && npm run build",
    "test": "npm run test:unit:default",
    "build:test": "webpack --config test/webpack.conf.js",
    "test:unit:default": "npm run build:test && karma start test/karma.conf.js --log-level debug --browsers PhantomJS"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^4.1.0",
    "css-loader": "^0.28.1",
    "file-loader": "^0.11.2",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "jupyter-js-widgets": "2.1.4",
    "select2": "^4.0.3",
    "underscore": "^1.8.3"
  }
}
